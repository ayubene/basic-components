{
  "name": "srit-basic-components",
  "version": "1.0.2",
  "description": "A Vue 3 component library based on Element Plus and vxe-table",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "node ./scripts/vue-tsc-patch.js --noEmit && vite build --mode lib",
    "build:lib": "vite build --mode lib",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,vue,js,jsx,json,css,scss,md}\"",
    "type-check": "node ./scripts/vue-tsc-patch.js --noEmit"
  },
  "keywords": [
    "vue",
    "vue3",
    "typescript",
    "element-plus",
    "vxe-table",
    "component-library"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "axios": "^1.0.0",
    "element-plus": "^2.4.0",
    "vue": "^3.3.0",
    "vxe-table": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "element-plus": "^2.4.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "^3.1.1",
    "sass": "^1.94.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.0",
    "vue": "^3.3.13",
    "vue-tsc": "^1.8.25",
    "vxe-table": "^4.0.0"
  },
  "dependencies": {
    "axios": "^1.0.0"
  }
}
